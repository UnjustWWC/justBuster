{
  "manifest_version": 2,
  "name": "Just Busters",
  "short_name": "Just Busters",
  "description": "Discourages the use of certain words that undermine women",
  "version": "1.0",
  "icons": {
    "16": "Favicon.png",
    "128": "Favicon Large.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "UnJust",
    "default_popup": "popup.html"
  }
}
